<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6//EN" "http://google-web-toolkit.googlecode.com/svn/releases/2.4/distro-source/core/src/gwt-module.dtd">
<module rename-to="app">
    <inherits name='com.google.gwt.user.UserAgent'/>
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="org.jboss.errai.common.ErraiCommon"/>
    <inherits name="org.jboss.errai.bus.ErraiBus"/>
    <inherits name="org.jboss.errai.ioc.Container"/>
    <inherits name="org.jboss.errai.databinding.DataBinding"/>
    <inherits name="org.jboss.errai.enterprise.CDI"/>
    <inherits name="org.jboss.errai.enterprise.Jaxrs"/>
    <inherits name="org.jboss.pressgang.ccms.rest.v1"/>
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name="hu.szaboaz.gwt.xslt.XSLT"/>
    <inherits name='edu.ycp.cs.dh.acegwt.AceGWT'/>
    <inherits name="com.google.gwt.user.client.ui"/>
    <inherits name='com.google.web.bindery.requestfactory.RequestFactory'/>
    <inherits name="org.vectomatic.libgwtfile"/>
    <!--http://forums.smartclient.com/showthread.php?t=20246 -->
    <inherits name="com.smartgwt.SmartGwtNoScript"/>

    <set-property name="gwt.logging.logLevel" value="INFO"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED"/>

    <!--  <set-property name="user.agent" value="safari,gecko1_8" /> -->

    <set-property name="compiler.stackMode" value="emulated"/>
    <set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="true"/>

    <!-- Super Dev Mode -->
    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>

    <!-- Logging level -->
    <!--<set-property name="gwt.logging.logLevel" value="SEVERE"/>-->
</module>
